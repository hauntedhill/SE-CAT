	<rowConstraints>
		<RowConstraints minHeight="10.0" prefHeight="30.0"
			vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<TitledPane animated="false" layoutX="31.0" layoutY="21.0" collapsible="false"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
