								<ImageView smooth="true" fitHeight="16" fitWidth="16"
									preserveRatio="true">
									<image>
										<Image url="@/image/icons/apply.png" />
									</image>
								</ImageView>
							</graphic>


						</Button>
						<ComboBox fx:id="boxSprachen" layoutX="170.0" layoutY="44.0"
							prefWidth="150.0" GridPane.columnIndex="1" />
						<Label layoutX="31.0" layoutY="48.0" prefHeight="17.0"
							prefWidth="157.0" text="%scene.sprache.lable" />
